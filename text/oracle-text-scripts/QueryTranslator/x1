<query>
  <textquery>
    <progression>
      <seq>DEFINEMERGE( ( (({brown}
{fox})),(({brown},{fox})) ), AND, ADD )</seq>
      <seq>DEFINEMERGE( (
(NEAR(({brown},{fox}))),(({brown},{fox})) ), AND, ADD )</seq>

<seq>DEFINEMERGE( ( (({brown}&{fox})),(({brown},{fox})) ), AND, ADD )</seq>

<seq>DEFINEMERGE( ( ((NEAR(({brown},{fox})))),(({brown},{fox})) ), AND, ADD
)</seq>
      <seq>((({brown},{fox})))</seq>
    </progression>
  </textquery>

<score datatype="FLOAT"/>
</query>
