DECLARE

salary number:=20000000;

BEGIN

IF salary > 100000  THEN dbms_output.put_line('Grade A');
ELSIF salary <= 50000 AND salary >= 100000 THEN dbms_output.put_line('Grade B'); 
ELSIF salary <= 25000 AND salary >= 500000 THEN dbms_output.put_line('Grade C'); 
ELSIF salary <= 10000 AND salary >= 250000 THEN dbms_output.put_line('Grade D'); 
ELSE dbms_output.put_line('Grade E');
END IF;

END;
/
