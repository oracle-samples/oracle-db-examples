ttice)\n    x &lt;- sample(num_dots_1)\n    y &lt;- sample(num_dots_1)\n    print(xyplot(y ~ x, main = \"Random Red Dots\", pch = 19, col = \"red\"))\n    x1 &lt;- sample(num_dots_2)\n    y1 &lt;- sample(num_dots_2)\n    print(xyplot(y1 ~ x1, pch = 19, col = \"red\", main = \"Random Red Dots\"))\n}                                                                                                                                                                                                                                                                        <\/span><\/td><\/tr>\n\t<tr><td><span style=white-space:pre-wrap>RandomRedDots.2        <\/span><\/td><td><span style=white-space:pre-wrap>function (num_dots_1 = 100, num_dots_2 = 10) \n{\n    library(lattice)\n    x &lt;- sample(num_dots_1)\n    y &lt;- sample(num_dots_1)\n    fig1 &lt;- xyplot(y ~ x, pch = 19, col = \"red\", main = \"Random Red Dots\")\n    print(fig1)\n    x1 &lt;- sample(num_dots_2)\n    y1 &lt;- sample(num_dots_2)\n    fig2 &lt;- xyplot(y1 ~ x1, pch = 19, col = \"red\", main = \"Random Red Dots\")\n    print(fig2)\n}                                                                                                                                                                                                                                    <\/span><\/td><\/tr>\n\t<tr><td><span style=white-space:pre-wrap>RandomRedDots.3        <\/span><\/td><td><span style=white-space:pre-wrap>function (num_dots_1 = 100, num_dots_2 = 10) \n{\n    library(lattice)\n    fig1 &lt;- xyplot(sample(num_dots_1) ~ sample(num_dots_1), pch = 19, col = \"red\", main = \"Random Red Dots\")\n    fig2 &lt;- xyplot(sample(num_dots_2) ~ sample(num_dots_2), pch = 19, col = \"red\", main = \"Random Red Dots\")\n    print(fig1)\n    print(fig2)\n}                                                                                                                                                                                                                                                                                        <\/span><\/td><\/tr>\n\t<tr><td><span style=white-space:pre-wrap>RandomRedDots2         <\/span><\/td><td><span style=white-space:pre-wrap>function(num_dots_1=100, num_dots_2=10){\n            id &lt;- 1:10\n            plot( 1:num_dots_1, rnorm(num_dots_1), pch = 21, \n                  bg = \"red\", cex = 2, main=\"Random Red Dots\" )\n            plot( 1:num_dots_2, rnorm(num_dots_2), pch = 21, \n                  bg = \"red\", cex = 2, main=\"Random Red Dots\" )\n            data.frame(id=id, val=id / 100)\n            }                                                                                                                                                                                                                                   <\/span><\/td><\/tr>\n\t<tr><td><span style=white-space:pre-wrap>build.lmRestR          <\/span><\/td><td><span style=white-space:pre-wrap>function (dat) \n{\n    dat$Species &lt;- as.factor(dat$Species)\n    mod &lt;- lm(Petal.Length ~ ., dat)\n    res &lt;- summary(mod)\n    res.list &lt;- list(Coef = res$coefficients, R2 = res$r.squared)\n    res.list\n}                                                                                                                                                                                                                                                                                                                                                                                                               <\/span><\/td><\/tr>\n\t<tr><td><span style=white-space:pre-wrap>buildLM.1              <\/span><\/td><td><span style=white-space:pre-wrap>function (dat, dsname) \n{\n    library(lattice)\n    regr &lt;- lm(Petal.Length ~ Petal.Width, dat)\n    x &lt;- dat[[\"Petal.Width\"]]\n    y &lt;- dat[[\"Petal.Length\"]]\n    xyplot(y ~ x, xlab = \"Petal Width\", ylab = \"Petal Length\", panel = function(x, y) {\n        panel.dotplot(x, y)\n        panel.abline(lm(y ~ x))\n    }, xlim = c(0, 2.7), ylim = c(0, 7.5), main = \"Prediction of Petal Length\")\n    return(regr)\n}                                                                                                                                                                                                  <\/span><\/td><\/tr>\n\t<tr><td><span style=white-space:pre-wrap>buildLM.2              <\/span><\/td><td><span style=white-space:pre-wrap>function (dat, dsname) \n{\n    library(lattice)\n    mod &lt;- lm(Petal.Length ~ Petal.Width, dat)\n    ore.save(mod, name = dsname, overwrite = TRUE)\n    return(mod)\n}                                                                                                                                                                                                                                                                                                                                                                                                                                                       <\/span><\/td><\/tr>\n\t<tr><td><span style=white-space:pre-wrap>buildLM.group          <\/span><\/td><td><span style=white-space:pre-wrap>function (dat) \n{\n    library(lattice)\n    mod &lt;- lm(Petal.Length ~ Petal.Width, dat)\n    return(mod)\n}                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   <\/span><\/td><\/tr>\n\t<tr><td><span style=white-space:pre-wrap>buildNBmodel           <\/span><\/td><td><span style=white-space:pre-wrap>function (dat, dsname) \n{\n    library(e1071)\n    dat$Species &lt;- as.factor(dat$Species)\n    mod &lt;- naiveBayes(Species ~ ., dat)\n    ore.save(mod, name = dsname)\n    TRUE\n}                                                                                                                                                                                                                                                                                                                                                                                                                                              <\/span><\/td><\/tr>\n\t<tr><td><span style=white-space:pre-wrap>group.countR           <\/span><\/td><td><span style=white-space:pre-wrap>function (df) \n{\n    x &lt;- aggregate(df[, \"Species\"], by = list(df$Species), FUN = length)\n    names(x) &lt;- c(\"SPECIES\", \"COUNT\")\n    return(x)\n}                                                                                                                                                                                                                                                                                                                                                                                                                                                                           <\/span><\/td><\/tr>\n\t<tr><td><span style=white-space:pre-wrap>multi.forecast         <\/span><\/td><td><span style=white-space:pre-wrap>function () \n{\n    library(forecast)\n    library(ggplot2)\n    data(\"AirPassengers\")\n    air.train &lt;- window(AirPassengers, end = 1956 + 11/12)\n    air.test &lt;- window(AirPassengers, start = 1957)\n    n.test &lt;- length(air.test)\n    air.model &lt;- auto.arima(air.train)\n    accuracy(air.model)\n    air.multi.forecast &lt;- air.model %&gt;% forecast(h = n.test)\n    air.multi.forecast %&gt;% autoplot()\n    res &lt;- accuracy(air.multi.forecast, x = air.test)\n    print(air.multi.forecast %&gt;% autoplot() + geom_line(aes(x = as.numeric(time(air.test)), y = as.numeric(air.test)), col = \"red\"))\n    return(res)\n}<\/span><\/td><\/tr>\n\t<tr><td>myLinearRegressionModel<\/td><td><span style=white-space:pre-wrap>function(dat, dsname){\n       mod &lt;- lm(Petal.Length~Petal.Width, dat)\n       ore.save(mod, name=dsname, overwrite = TRUE)\n       return(TRUE)\n      }                                                                                                                                                                                                                                                                                                                                                                                                                                                                     <\/span><\/td><\/tr>\n\t<tr><td><span style=white-space:pre-wrap>scoreData              <\/span><\/td><td><span style=white-space:pre-wrap>function (dat, dsname) \n{\n    ore.load(dsname)\n    res &lt;- data.frame(IS_CHURNER = dat$IS_CHURNER)\n    res$PREDICTION &lt;- predict(rpart.mod.r, dat)\n    res\n}                                                                                                                                                                                                                                                                                                                                                                                                                                                              <\/span><\/td><\/tr>\n\t<tr><td><span style=white-space:pre-wrap>scoreLM.1              <\/span><\/td><td><span style=white-space:pre-wrap>function (dat, dsname) \n{\n    library(lattice)\n    ore.load(dsname)\n    dat$Petal.Length_prediction &lt;- predict(mod, newdata = dat)\n    dat[, c(\"Petal.Length_prediction\", \"Petal.Length\", \"Species\")]\n}                           