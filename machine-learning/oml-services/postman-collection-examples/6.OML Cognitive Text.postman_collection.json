							"        <tr>",
							"            <th>Relationship</th>",
							"            <th>Hypertext Reference</th>",
							"        </tr>",
							"    {{#each links}}",
							"        <tr>",
							"            <td>{{rel}}</td>",
							"            <td>{{href}}</td>",
							"        </tr>",
							"    {{/each}} ",
							"    </table>",
							"",
